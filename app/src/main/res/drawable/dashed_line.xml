<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:pivotX="50%"
    android:pivotY="50%"
    android:fromDegrees="90">
    <shape
        android:shape="line">
        <stroke
            android:width="1dp"
            android:color="@color/primary"
            android:dashWidth="4dp"
            android:dashGap="2dp" />
        <size
            android:width="1dp"
            android:height="24dp" />
    </shape>
</rotate>